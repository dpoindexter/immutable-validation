{
  "name": "immutable-validation",
  "version": "0.5.0",
  "description": "React form validation using top-down, immutable data",
  "main": "dist",
  "scripts": {
    "test": "karma start",
    "test:ci": "karma start --no-single-run --browsers Chrome --auto-watch",
    "build": "webpack src/validation.js dist/ImmutableValidation.min.js",
    "build:dev": "webpack src/validation.js dist/ImmutableValidation.js --config webpack.config.dev.js",
    "prepublish": "npm test && npm run build && npm run build:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpoindexter/immutable-validation.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "javascript",
    "immutable",
    "immutablejs",
    "form",
    "validation"
  ],
  "author": "Daniel Poindexter <dp.texas@gmail.com> (http://github.com/dpoindexter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpoindexter/immutable-validation/issues"
  },
  "homepage": "https://github.com/dpoindexter/immutable-validation#readme",
  "peerDependencies": {
    "react": ">=0.13.0 <0.15.0 || 0.14.0-beta1",
    "immutable": ">=3.0.0"
  },
  "devDependencies": {
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "eslint": "^2.2.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "webpack": "^1.11.0"
  }
}
